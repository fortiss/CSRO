@prefix : <https://w3id.org/csro/ontology#> .
@prefix cc: <http://creativecommons.org/ns#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix csro: <https://w3id.org/csro/ontology#> .
@prefix d3f: <http://d3fend.mitre.org/ontologies/d3fend.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix terms: <http://purl.org/dc/terms/> .
@prefix schema: <http://schema.org/> .
@base <https://w3id.org/csro/ontology#> .

<https://w3id.org/csro/ontology> rdf:type owl:Ontology ;
                              owl:imports <http://d3fend.mitre.org/ontologies/d3fend.owl> ;
                              cc:license "https://creativecommons.org/licenses/by/4.0/" ;
                              dc:contributor "Contributors and reviewers from the container security community"@en ;
                              dc:creator "Yannick Landeck"@en ;
                              dc:description "The Container Security Risk Ontology (CSRO) provides a formal vocabulary for modeling security risks in containerized deployments, particularly in operational technology (OT) contexts. This ontology includes both the conceptual schema and concrete instances with enhancements for attack techniques and dynamic risk assessment. Integrated with MITRE D3FEND ontology for ATT&CK technique references."@en ;
                              dc:issued "2025-09-24"^^xsd:date ;
                              dc:modified "2025-09-24T00:00:00Z" ;
                              dc:publisher "Yannick Landeck"@en ;
                              dc:title "Container Security Risk Ontology"@en ;
                              terms:created "24th September 2025"@en ;
                              vann:preferredNamespacePrefix "csro" ;
                              schema:url "https://w3id.org/csro/ontology"^^xsd:anyURI ;
                              schema:version "1.0" ;
                              owl:versionInfo "1.0" ;
                              skos:definition "A comprehensive ontology for assessing and modeling security risks in container deployments using context scenarios and attack patterns for dynamic risk calculation."@en .

#################################################################
#    Annotation properties
#################################################################

###  http://creativecommons.org/ns#license
cc:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/contributor
dc:contributor rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/issued
dc:issued rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/modified
dc:modified rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/publisher
dc:publisher rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/created
terms:created rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://schema.org/url
schema:url rdf:type owl:AnnotationProperty .


###  http://schema.org/version
schema:version rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


###  https://w3id.org/csro/ontology/ontology#effectDescription
csro:effectDescription rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


###  http://www.w3.org/2001/XMLSchema#gYear
xsd:gYear rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/csro/ontology#addresses
csro:addresses rdf:type owl:ObjectProperty ;
              rdfs:domain csro:RiskTreatment ;
              rdfs:range csro:SecurityAssumption ;
              rdfs:comment "Indicates that a risk treatment addresses or implements a specific security assumption."@en ;
              rdfs:label "addresses"@en .


###  https://w3id.org/csro/ontology#affects
csro:affects rdf:type owl:ObjectProperty ;
            rdfs:domain csro:AttackAction ;
            rdfs:range csro:Component ;
            rdfs:comment "Indicates what components are affected by an attack action."@en ;
            rdfs:label "affects"@en .


###  https://w3id.org/csro/ontology#appliesTechnique
csro:appliesTechnique rdf:type owl:ObjectProperty ;
                     rdfs:domain csro:AttackAction ;
                     rdfs:range csro:ContainerAttackTechnique ;
                     rdfs:comment "Indicates that an attack action applies a specific container attack technique."@en ;
                     rdfs:label "applies technique"@en .


###  https://w3id.org/csro/ontology#appliesTo
csro:appliesTo rdf:type owl:ObjectProperty ;
              rdfs:domain csro:AttackPatternCalculationRule ;
              rdfs:comment "Indicates which attack pattern a calculation rule applies to."@en ;
              rdfs:label "applies to"@en .


###  https://w3id.org/csro/ontology#belongsToCategory
csro:belongsToCategory rdf:type owl:ObjectProperty ;
                      rdfs:domain csro:SecurityAssumption ;
                      rdfs:range csro:AssumptionCategory ;
                      rdfs:comment "Indicates the category to which a security assumption belongs."@en ;
                      rdfs:label "belongs to category"@en .


###  https://w3id.org/csro/ontology#belongsToStandard
csro:belongsToStandard rdf:type owl:ObjectProperty ;
                      rdfs:domain csro:ContainerSecurityStandardSection ;
                      rdfs:range csro:ContainerSecurityStandard ;
                      rdfs:comment "Indicates which security standard a section belongs to."@en ;
                      rdfs:label "belongs to standard"@en .


###  https://w3id.org/csro/ontology#causesImpact
csro:causesImpact rdf:type owl:ObjectProperty ;
                 rdfs:domain csro:AttackAction ;
                 rdfs:range csro:Impact ;
                 rdfs:comment "Indicates the security impact that results from an attack action being successful."@en ;
                 rdfs:label "causes impact"@en .


###  https://w3id.org/csro/ontology#forAssumption
csro:forAssumption rdf:type owl:ObjectProperty ;
                  rdfs:domain csro:AssumptionInScenario ;
                  rdfs:range csro:SecurityAssumption ;
                  rdfs:comment "Indicates which security assumption an assumption-in-scenario instance refers to."@en ;
                  rdfs:label "for assumption"@en .


###  https://w3id.org/csro/ontology#hasBaseDifficulty
csro:hasBaseDifficulty rdf:type owl:ObjectProperty ;
                      rdfs:range csro:DifficultyLevel ;
                      rdfs:comment "Indicates the base difficulty level of executing an attack pattern."@en ;
                      rdfs:label "has base difficulty"@en .


###  https://w3id.org/csro/ontology#hasExploitabilityRating
csro:hasExploitabilityRating rdf:type owl:ObjectProperty ;
                            rdfs:comment "Associates an entity with its exploitability rating."@en ;
                            rdfs:label "has exploitability rating"@en .


###  https://w3id.org/csro/ontology#hasExposureRating
csro:hasExposureRating rdf:type owl:ObjectProperty ;
                      rdfs:comment "Associates an entity with its exposure rating."@en ;
                      rdfs:label "has exposure rating"@en .


###  https://w3id.org/csro/ontology#hasGuideline
csro:hasGuideline rdf:type owl:ObjectProperty ;
                 rdfs:domain csro:RiskTreatment ;
                 rdfs:range csro:ContainerSecurityGuideline ;
                 rdfs:comment "Associates a risk treatment with applicable security guidelines."@en ;
                 rdfs:label "has guideline"@en .


###  https://w3id.org/csro/ontology#hasImpactRating
csro:hasImpactRating rdf:type owl:ObjectProperty ;
                    rdfs:domain csro:Impact ;
                    rdfs:range csro:ImpactRating ;
                    rdfs:comment "Associates an impact with its severity rating."@en ;
                    rdfs:label "has impact rating"@en .


###  https://w3id.org/csro/ontology#hasLikelihood
csro:hasLikelihood rdf:type owl:ObjectProperty ;
                  rdfs:domain csro:AttackAction ;
                  rdfs:range csro:Likelihood ;
                  rdfs:comment "Associates an attack action with its calculated likelihood rating."@en ;
                  rdfs:label "has likelihood"@en .


###  https://w3id.org/csro/ontology#hasLikelihoodRating
csro:hasLikelihoodRating rdf:type owl:ObjectProperty ;
                        rdfs:domain csro:RiskCalculationRule ;
                        rdfs:range csro:Likelihood ;
                        rdfs:comment "Associates a risk calculation rule with a likelihood rating."@en ;
                        rdfs:label "has likelihood rating"@en .


###  https://w3id.org/csro/ontology#hasRiskLevel
csro:hasRiskLevel rdf:type owl:ObjectProperty ;
                 rdfs:domain csro:Risk ;
                 rdfs:range csro:RiskLevel ;
                 rdfs:comment "Associates a risk with its calculated risk level."@en ;
                 rdfs:label "has risk level"@en .


###  https://w3id.org/csro/ontology#hasSatisfactionState
csro:hasSatisfactionState rdf:type owl:ObjectProperty ;
                         rdfs:domain csro:AssumptionInScenario ;
                         rdfs:range csro:SatisfactionState ;
                         rdfs:comment "Indicates the satisfaction state of a security assumption within a context scenario."@en ;
                         rdfs:label "has satisfaction state"@en .


###  https://w3id.org/csro/ontology#hasWeight
csro:hasWeight rdf:type owl:ObjectProperty ;
              rdfs:domain csro:AttackPatternCalculationRule ;
              rdfs:range csro:AssumptionWeight ;
              rdfs:comment "Associates a calculation rule with assumption weights."@en ;
              rdfs:label "has weight"@en .


###  https://w3id.org/csro/ontology#inContext
csro:inContext rdf:type owl:ObjectProperty ;
              rdfs:domain csro:AttackAction ;
              rdfs:range csro:ContextScenario ;
              rdfs:comment "Indicates the context scenario in which an attack action is evaluated."@en ;
              rdfs:label "in context"@en .


###  https://w3id.org/csro/ontology#includes
csro:includes rdf:type owl:ObjectProperty ;
             rdfs:domain csro:ContextScenario ;
             rdfs:range csro:Component ;
             rdfs:comment "Indicates which components are involved in or affected by a context scenario."@en ;
             rdfs:label "includes"@en .


###  https://w3id.org/csro/ontology#includesAssumption
csro:includesAssumption rdf:type owl:ObjectProperty ;
                       rdfs:domain csro:ContextScenario ;
                       rdfs:range csro:SecurityAssumption ;
                       rdfs:comment "Indicates that a context scenario includes a specific security assumption."@en ;
                       rdfs:label "includes assumption"@en .


###  https://w3id.org/csro/ontology#indicates
csro:indicates rdf:type owl:ObjectProperty ;
              rdfs:domain csro:Impact ;
              rdfs:range csro:Risk ;
              rdfs:comment "Indicates that an impact represents or points to a specific risk."@en ;
              rdfs:label "indicates"@en .


###  https://w3id.org/csro/ontology#isDerivedFrom
csro:isDerivedFrom rdf:type owl:ObjectProperty ;
                  rdfs:domain csro:ContainerDeploymentTrait ;
                  rdfs:range csro:ContainerEvidence ;
                  rdfs:comment "Indicates that a container deployment trait is derived from or verified by specific evidence."@en ;
                  rdfs:label "is derived from"@en .


###  https://w3id.org/csro/ontology#isImplementedBy
csro:isImplementedBy rdf:type owl:ObjectProperty ;
                    rdfs:domain csro:RiskTreatment ;
                    rdfs:range csro:ContainerDeploymentTrait ;
                    rdfs:comment "Indicates the container deployment traits that implement a risk treatment."@en ;
                    rdfs:label "is implemented by"@en .


###  https://w3id.org/csro/ontology#isTreatedBy
csro:isTreatedBy rdf:type owl:ObjectProperty ;
                owl:inverseOf csro:treats ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( csro:AttackAction
                                            csro:Risk
                                          )
                            ] ;
                rdfs:range csro:RiskTreatment ;
                rdfs:comment "Indicates that a risk or attack action is addressed by a specific treatment."@en ;
                rdfs:label "is treated by"@en .


###  https://w3id.org/csro/ontology#isVerifiableBy
csro:isVerifiableBy rdf:type owl:ObjectProperty ;
                   rdfs:domain csro:SecurityAssumption ;
                   rdfs:range csro:ContainerDeploymentTrait ;
                   rdfs:comment "Indicates that a security assumption can be verified by the presence of specific container deployment traits."@en ;
                   rdfs:label "is verifiable by"@en .


###  https://w3id.org/csro/ontology#isVerifiableByAbsence
csro:isVerifiableByAbsence rdf:type owl:ObjectProperty ;
                          rdfs:domain csro:SecurityAssumption ;
                          rdfs:range csro:ContainerDeploymentTrait ;
                          rdfs:comment "Indicates that a security assumption can be verified by the absence of specific container deployment traits."@en ;
                          rdfs:label "is verifiable by absence"@en .


###  https://w3id.org/csro/ontology#originatesFrom
csro:originatesFrom rdf:type owl:ObjectProperty ;
                   rdfs:domain csro:SecurityAssumption ;
                   rdfs:range [ rdf:type owl:Class ;
                                owl:unionOf ( csro:ContainerSecurityStandard
                                              csro:ContainerSecurityStandardSection
                                            )
                              ] ;
                   rdfs:comment "Indicates that a security assumption originates from or is defined by a specific security standard or section."@en ;
                   rdfs:label "originates from"@en .


###  https://w3id.org/csro/ontology#referencesAttackTechnique
csro:referencesAttackTechnique rdf:type owl:ObjectProperty ;
                              rdfs:domain csro:ContainerAttackTechnique ;
                              rdfs:range d3f:ATTACKEnterpriseTechnique ;
                              rdfs:comment "References the MITRE ATT&CK Enterprise technique that this container attack technique is based on."@en ;
                              rdfs:label "references attack technique"@en .


###  https://w3id.org/csro/ontology#refersToAssumption
csro:refersToAssumption rdf:type owl:ObjectProperty ;
                       rdfs:domain csro:AssumptionWeight ;
                       rdfs:range csro:SecurityAssumption ;
                       rdfs:comment "Indicates which security assumption an assumption weight refers to."@en ;
                       rdfs:label "refers to assumption"@en .


###  https://w3id.org/csro/ontology#requiresTrait
csro:requiresTrait rdf:type owl:ObjectProperty ;
                  rdfs:range csro:ContainerDeploymentTrait ;
                  rdfs:comment "Indicates the container deployment traits required for an attack pattern to be applicable."@en ;
                  rdfs:label "requires trait"@en .


###  https://w3id.org/csro/ontology#resultsIn
csro:resultsIn rdf:type owl:ObjectProperty ;
              rdfs:domain csro:AttackPatternCalculationRule ;
              rdfs:comment "Indicates the result produced by a calculation rule."@en ;
              rdfs:label "results in"@en .


###  https://w3id.org/csro/ontology#treats
csro:treats rdf:type owl:ObjectProperty ;
           rdfs:domain csro:RiskTreatment ;
           rdfs:range csro:Risk ;
           rdfs:comment "Indicates that a risk treatment addresses or mitigates a specific risk."@en ;
           rdfs:label "treats"@en .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/csro/ontology#assumptionId
csro:assumptionId rdf:type owl:DatatypeProperty ;
                 rdfs:domain csro:SecurityAssumption ;
                 rdfs:range xsd:string ;
                 rdfs:comment "The unique identifier for a security assumption (e.g., RTS-2, IMG-2)."@en ;
                 rdfs:label "assumption ID"@en .


###  https://w3id.org/csro/ontology#description
csro:description rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:string ;
                rdfs:comment "Provides a detailed textual description of the concept."@en ;
                rdfs:label "description"@en .


###  https://w3id.org/csro/ontology#publicationYear
csro:publicationYear rdf:type owl:DatatypeProperty ;
                    rdfs:domain csro:ContainerSecurityStandard ;
                    rdfs:range xsd:gYear ;
                    rdfs:comment "The year when the security standard was published."@en ;
                    rdfs:label "publication year"@en .


###  https://w3id.org/csro/ontology#publishedBy
csro:publishedBy rdf:type owl:DatatypeProperty ;
                rdfs:domain csro:ContainerSecurityStandard ;
                rdfs:range xsd:string ;
                rdfs:comment "The organization that published or maintains the security standard."@en ;
                rdfs:label "published by"@en .


###  https://w3id.org/csro/ontology#referenceUrl
csro:referenceUrl rdf:type owl:DatatypeProperty ;
                 rdfs:range xsd:anyURI ;
                 rdfs:comment "Provides a reference URL or web link for additional information about the concept."@en ;
                 rdfs:label "reference URL"@en .


###  https://w3id.org/csro/ontology#sectionId
csro:sectionId rdf:type owl:DatatypeProperty ;
              rdfs:domain csro:ContainerSecurityStandardSection ;
              rdfs:range xsd:string ;
              rdfs:comment "The identifier of the section within the standard (e.g., '§4.1.2', '4.3', 'D04', '#9')."@en ;
              rdfs:label "section ID"@en .


###  https://w3id.org/csro/ontology#shortDescription
csro:shortDescription rdf:type owl:DatatypeProperty ;
                     rdfs:range xsd:string ;
                     rdfs:comment "Provides a brief textual description of the concept."@en ;
                     rdfs:label "short description"@en .


###  https://w3id.org/csro/ontology#standardId
csro:standardId rdf:type owl:DatatypeProperty ;
               rdfs:domain csro:ContainerSecurityStandard ;
               rdfs:range xsd:string ;
               rdfs:comment "The unique identifier or name of the security standard."@en ;
               rdfs:label "standard ID"@en .


###  https://w3id.org/csro/ontology#version
csro:version rdf:type owl:DatatypeProperty ;
            rdfs:domain csro:ContainerSecurityStandard ;
            rdfs:range xsd:string ;
            rdfs:comment "The version number or identifier of the security standard."@en ;
            rdfs:label "version"@en .


###  https://w3id.org/csro/ontology#weightValue
csro:weightValue rdf:type owl:DatatypeProperty ;
                rdfs:domain csro:AssumptionWeight ;
                rdfs:range xsd:decimal ;
                rdfs:comment "The numerical weight value indicating the effectiveness of an assumption (0-3 scale)."@en ;
                rdfs:label "weight value"@en .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/csro/ontology#AssumptionCategory
csro:AssumptionCategory rdf:type owl:Class ;
                       rdfs:subClassOf csro:ContainerSecurityRiskThing ;
                       rdfs:comment "A category for grouping related security assumptions (e.g., Runtime Security, Network Security)."@en ;
                       rdfs:label "Assumption Category"@en .


###  https://w3id.org/csro/ontology#AssumptionInScenario
csro:AssumptionInScenario rdf:type owl:Class ;
                         rdfs:subClassOf csro:ContainerSecurityRiskThing ;
                         rdfs:comment "A specific instance of a security assumption within a context scenario, including its satisfaction state."@en ;
                         rdfs:label "Assumption in Scenario"@en .


###  https://w3id.org/csro/ontology#AssumptionWeight
csro:AssumptionWeight rdf:type owl:Class ;
                     rdfs:subClassOf csro:ContainerSecurityRiskThing ;
                     rdfs:comment "The weight or effectiveness value of a security assumption in relation to a specific calculation rule."@en ;
                     rdfs:label "Assumption Weight"@en .


###  https://w3id.org/csro/ontology#AttackAction
csro:AttackAction rdf:type owl:Class ;
                 rdfs:subClassOf csro:ContainerSecurityRiskThing ;
                 rdfs:comment "The application of an attack pattern in a specific context scenario, with dynamically calculated risk assessments."@en ;
                 rdfs:label "Attack Action"@en .


###  https://w3id.org/csro/ontology#AttackPatternCalculationRule
csro:AttackPatternCalculationRule rdf:type owl:Class ;
                                 rdfs:subClassOf csro:ContainerSecurityRiskThing ;
                                 rdfs:comment "A rule for dynamically calculating risk assessment values based on attack patterns and context scenarios."@en ;
                                 rdfs:label "Attack Pattern Calculation Rule"@en .


###  https://w3id.org/csro/ontology#Component
csro:Component rdf:type owl:Class ;
              rdfs:subClassOf csro:ContainerSecurityRiskThing ;
              rdfs:comment "A system component that can be affected by security impacts."@en ;
              rdfs:label "Component"@en .


###  https://w3id.org/csro/ontology#ContainerAttackTechnique
csro:ContainerAttackTechnique rdf:type owl:Class ;
                             rdfs:subClassOf csro:ContainerSecurityRiskThing ;
                             rdfs:comment "A MITRE ATT&CK technique adapted and contextualized for container security risk assessment."@en ;
                             rdfs:label "Container Attack Technique"@en .


###  https://w3id.org/csro/ontology#ContainerDeploymentTrait
csro:ContainerDeploymentTrait rdf:type owl:Class ;
                             rdfs:subClassOf csro:ContainerSecurityRiskThing ;
                             rdfs:comment "A configuration characteristic or setting used in container deployment."@en ;
                             rdfs:label "Container Deployment Trait"@en .


###  https://w3id.org/csro/ontology#ContainerEvidence
csro:ContainerEvidence rdf:type owl:Class ;
                      rdfs:subClassOf csro:ContainerSecurityRiskThing ;
                      rdfs:comment "Evidence that can be collected to verify container deployment traits and security assumptions."@en ;
                      rdfs:label "Container Evidence"@en .


###  https://w3id.org/csro/ontology#ContainerSecurityAssumption
csro:ContainerSecurityAssumption rdf:type owl:Class ;
                                rdfs:subClassOf csro:SecurityAssumption ;
                                rdfs:comment "A security assumption about the operational environment of a container application, derived from standards and best practices."@en ;
                                rdfs:label "Container Security Assumption"@en .


###  https://w3id.org/csro/ontology#ContainerSecurityGuideline
csro:ContainerSecurityGuideline rdf:type owl:Class ;
                               rdfs:subClassOf csro:ContainerSecurityRiskThing ;
                               rdfs:comment "A security guideline or best practice recommendation for container deployments."@en ;
                               rdfs:label "Container Security Guideline"@en .


###  https://w3id.org/csro/ontology#ContainerSecurityRiskThing
csro:ContainerSecurityRiskThing rdf:type owl:Class ;
                               rdfs:comment "The root class for all risk assessment concepts in container security."@en ;
                               rdfs:label "Container Security Risk Thing"@en .


###  https://w3id.org/csro/ontology#ContainerSecurityStandard
csro:ContainerSecurityStandard rdf:type owl:Class ;
                              rdfs:subClassOf csro:ContainerSecurityRiskThing ;
                              rdfs:comment "A formal security standard or framework that defines container security requirements."@en ;
                              rdfs:label "Container Security Standard"@en .


###  https://w3id.org/csro/ontology#ContainerSecurityStandardSection
csro:ContainerSecurityStandardSection rdf:type owl:Class ;
                                     rdfs:subClassOf csro:ContainerSecurityRiskThing ;
                                     rdfs:comment "A specific section, chapter, or control within a container security standard."@en ;
                                     rdfs:label "Container Security Standard Section"@en .


###  https://w3id.org/csro/ontology#ContextScenario
csro:ContextScenario rdf:type owl:Class ;
                    rdfs:subClassOf csro:ContainerSecurityRiskThing ;
                    rdfs:comment "A scenario representing a collection of security assumptions with their satisfaction states. Scenarios are independent of specific attacks and represent different security postures."@en ;
                    rdfs:label "Context Scenario"@en .


###  https://w3id.org/csro/ontology#DifficultyLevel
csro:DifficultyLevel rdf:type owl:Class ;
                    rdfs:subClassOf csro:ContainerSecurityRiskThing ;
                    rdfs:comment "The base difficulty level for executing an attack pattern (e.g., Low, Medium, High)."@en ;
                    rdfs:label "Difficulty Level"@en .


###  https://w3id.org/csro/ontology#DockerComposeSetting
csro:DockerComposeSetting rdf:type owl:Class ;
                         rdfs:subClassOf csro:ContainerEvidence ;
                         rdfs:comment "Configuration settings extracted from Docker Compose files as evidence for container deployment traits."@en ;
                         rdfs:label "Docker Compose Setting"@en .


###  https://w3id.org/csro/ontology#ExploitabilityCalculationRule
csro:ExploitabilityCalculationRule rdf:type owl:Class ;
                                  rdfs:subClassOf csro:AttackPatternCalculationRule ;
                                  rdfs:comment "A rule for calculating exploitability ratings based on attack patterns and security assumptions."@en ;
                                  rdfs:label "Exploitability Calculation Rule"@en .


###  https://w3id.org/csro/ontology#ExploitabilityRating
csro:ExploitabilityRating rdf:type owl:Class ;
                         rdfs:subClassOf csro:ContainerSecurityRiskThing ;
                         rdfs:comment "A rating indicating the ease of exploitation (e.g., Low, Medium, High)."@en ;
                         rdfs:label "Exploitability Rating"@en .


###  https://w3id.org/csro/ontology#ExposureCalculationRule
csro:ExposureCalculationRule rdf:type owl:Class ;
                            rdfs:subClassOf csro:AttackPatternCalculationRule ;
                            rdfs:comment "A rule for calculating exposure ratings based on attack patterns and security assumptions."@en ;
                            rdfs:label "Exposure Calculation Rule"@en .


###  https://w3id.org/csro/ontology#ExposureRating
csro:ExposureRating rdf:type owl:Class ;
                   rdfs:subClassOf csro:ContainerSecurityRiskThing ;
                   rdfs:comment "A rating indicating the level of exposure to attack (e.g., Low, Medium, High)."@en ;
                   rdfs:label "Exposure Rating"@en .


###  https://w3id.org/csro/ontology#HadolintDockerfileRule
csro:HadolintDockerfileRule rdf:type owl:Class ;
                           rdfs:subClassOf csro:ContainerEvidence ;
                           rdfs:comment "Dockerfile analysis rules and findings from Hadolint tool as evidence for container image security."@en ;
                           rdfs:label "Hadolint Dockerfile Rule"@en .


###  https://w3id.org/csro/ontology#Impact
